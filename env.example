# Frontend Environment Variables
VITE_API_BASE_URL=http://localhost:3001
VITE_PATIENT_IDENTITY_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_ENCRYPTED_NFT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
VITE_HEDERA_RPC_URL=https://testnet.hashio.io/api
VITE_PINATA_API_KEY=your_pinata_api_key_here
VITE_PINATA_SECRET_KEY=your_pinata_secret_key_here

# Backend Environment Variables (for Node.js backend)
NODE_ENV=development
PORT=3001
HEDERA_RPC_URL=https://testnet.hashio.io/api
ENCRYPTED_NFT_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
PATIENT_IDENTITY_CONTRACT_ADDRESS=0x0000000000000000000000000000000000000000
MINTER_PRIVATE_KEY=your_minter_private_key_here
REENCRYPTOR_PRIVATE_KEY=your_reencryptor_private_key_here
PINATA_API_KEY=your_pinata_api_key_here
PINATA_SECRET_KEY=your_pinata_secret_key_here
REDIS_URL=redis://localhost:6379
JWT_SECRET=your_jwt_secret_here

# Database (if using)
DATABASE_URL=postgresql://username:password@localhost:5432/medvault

# IPFS Configuration
IPFS_GATEWAY_URL=https://gateway.pinata.cloud/ipfs/
IPFS_API_URL=https://api.pinata.cloud

# Blockchain Network Configuration
NETWORK_NAME=hedera_testnet
CHAIN_ID=296
GAS_LIMIT=1000000
GAS_PRICE=1000000000

# Security Configuration
ENCRYPTION_ALGORITHM=AES-256-GCM
KEY_WRAPPING_ALGORITHM=RSA-OAEP
KEY_SIZE=2048
HASH_ALGORITHM=SHA-256

# Monitoring and Logging
LOG_LEVEL=info
ENABLE_METRICS=true
ENABLE_HEALTH_CHECKS=true

# Agent Configuration
REENCRYPTION_AGENT_ENABLED=true
AGENT_POLL_INTERVAL=30000
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=5000
